void setup() {
  // put your setup code here, to run once:
  while(!Serial);
  Serial.begin(9600);
  Serial1.begin(9600);
  delay(1000);
  
  Serial1.flush();
  Serial1.read();
  Serial.flush();
  Serial.read();
}

void loop() {
  // put your main code here, to run repeatedly: 
  Serial1.write("Deez Nuts");
  char x=Serial1.read();
  Serial.write(x);
  Serial.write("\nDeez Nuts\n");
  char y=Serial.read();
  Serial1.write(y);
}
